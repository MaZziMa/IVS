    "ivs",
    "chat",
    "profanity",
    "filter"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-ivschat": "^3.600.0"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18.x"
  }
}
