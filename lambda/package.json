{
  "name": "ivs-chat-profanity-filter",
  "version": "1.0.0",
  "description": "AWS Lambda function to filter profanity in IVS Chat messages",
  "main": "profanity-filter.js",
  "scripts": {
    "test": "node test-profanity-filter.js",
    "package": "zip -r profanity-filter.zip profanity-filter.js package.json node_modules/",
    "deploy": "aws lambda update-function-code --function-name ivs-chat-profanity-filter --zip-file fileb://profanity-filter.zip"
  },
  "keywords": [
    "aws",
